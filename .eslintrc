{
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "babel-eslint",
		"sourceType": "module",
		"allowImportExportEverywhere": false
	},
	"rules": {
		"quotes": ["error", "double"],
		"linebreak-style": [
			"error",
			"unix"
		]
	},
	"globals": {
		"console": true,
		"alert": true,
		"document": true,
		"window": true,
		"setTimeout": true,
		"setInterval": true,
		"clearTimeout": true
	}
}
